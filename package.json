{
  "name": "nmcp-api",
  "version": "2.0.46",
  "main": "dist/app.js",
  "repository": "https://github.com/AllenNeuralDynamics/nmcp-sample-api",
  "author": "Patrick Edson <pedson@leapscientific.com>",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "test": "jest",
    "debug": "tsc && node --trace-warnings --max-old-space-size=8192 --optimize-for-size src/app.js",
    "express": "node dist/app.js",
    "smartsheet": "npx run-func src/tools/smartSheetClient.js synchronize",
    "import-somas": "npx run-func src/tools/importSomaProperties.js importSomaProperties",
    "show-duplicate": "npx run-func src/util/processDuplicateReconstructions.js processDuplicateReconstructions"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "diagnostics": false
        }
      ]
    },
    "testRegex": "/__tests__/.*\\.(test|spec)\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.10.5",
    "@apollo/server": "^4.9.5",
    "byline": "^5.0.0",
    "cors": "^2.8.5",
    "csv": "^5.1.3",
    "csv-parse": "^4.8.6",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "glob": "^11.0.3",
    "global": "^4.4.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^15.0.2",
    "isomorphic-fetch": "^3.0.0",
    "JSONStream": "^1.3.5",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "^3.2.0",
    "node-worker-threads-pool": "^1.5.1",
    "pg": "^8.7.1",
    "sequelize": "^6.34.0",
    "smartsheet": "^4.0.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/graphql-upload": "^15.0.2",
    "@types/gulp": "4.0.6",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^12.12.6",
    "@types/uuid": "^3.4.5",
    "@types/validator": "^10.11.3",
    "jest": "^29.7.0",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.4.1",
    "typescript": "^5.3.3"
  }
}
